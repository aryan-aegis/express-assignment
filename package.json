{
  "type": "module",
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsdoc": "^4.0.0",
    "prisma": "^4.9.0",
    "serve": "^14.1.2"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "pretty": "prettier --write ./src",
    "lint": "eslint --ext .js ./src",
    "lint:fix": "eslint --ext .js src --fix",
    "prepare": "husky install",
    "generateDocs": "jsdoc -r ./src",
    "serveDocs": "pnpm generateDocs && serve out"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3"
  }
}
